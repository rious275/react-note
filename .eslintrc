{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "eslint"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "import/no-unresolved": "off"
  },
  "ignorePatterns": ["node_modules"]
}
